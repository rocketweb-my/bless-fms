{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "arielmejiadev/larapex-charts": "^2.1",
        "astrotomic/laravel-translatable": "^11.12",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-snappy": "^1.0",
        "botman/botman": "^2.6",
        "botman/driver-web": "^1.5",
        "doctrine/dbal": "^3.1.4",
        "guzzlehttp/guzzle": "^7.8",
        "intervention/image": "^3.0",
        "laminas/laminas-httphandlerrunner": "^2.0",
        "laravel/framework": "^11.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "league/flysystem-sftp-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mpdf/mpdf": "^8.0",
        "nyholm/psr7": "^1.2",
        "php-flasher/flasher-toastr-laravel": "^1.15",
        "predis/predis": "^2.0.2",
        "pusher/pusher-php-server": "^7.0",
        "ramsey/uuid-doctrine": "^2.0",
        "rap2hpoutre/fast-excel": "^5.5",
        "realrashid/sweet-alert": "^7.0",
        "symfony/cache": "^6.4",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/filesystem": "^6.4",
        "symfony/http-client": "^6.4",
        "symfony/mailgun-mailer": "^6.4",
        "symfony/postmark-mailer": "^6.4",
        "symfony/psr-http-message-bridge": "^6.4",
        "symfony/translation": "^6.4",
        "webklex/laravel-imap": "^5.0",
        "yajra/laravel-datatables-buttons": "^11.0",
        "yajra/laravel-datatables-export": "^11.0",
        "yajra/laravel-datatables-fractal": "^11.0",
        "yajra/laravel-datatables-html": "^11.0",
        "yajra/laravel-datatables-oracle": "^11.0"
    },
    "require-dev": {
        "brianium/paratest": "^7.3",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helpers.php",
            "app/Helpers/admin_helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
